{
  "name": "@gspenst/minimal-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "GSPENST_REVALIDATE='60000' tinacms build && next build",
    "dev": "tinacms dev -c \"next dev\"",
    "export": "GSPENST_STATIC_EXPORT=true npm run build && next export",
    "lint": "next lint",
    "start": "tinacms build && next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gspenst/next": "*",
    "@tinacms/cli": "^0.61",
    "gspenst": "*",
    "next": "^12.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "typescript": "4.6.3"
  }
}
