{
  "name": "@gspenst/blog-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "GSPENST_REVALIDATE='60000' tinacms build && next build",
    "debug": "NODE_OPTIONS='--inspect' npm dev",
    "dev": "tinacms dev -c \"next dev\"",
    "export": "GSPENST_STATIC_EXPORT=true npm run build && next export",
    "lint": "next lint",
    "start": "tinacms build && next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gspenst/next": "*",
    "@gspenst/nextra-theme-blog": "*",
    "@next/bundle-analyzer": "^12.2.2",
    "@tinacms/cli": "^0.61.1",
    "gspenst": "*",
    "next": "^12.2.0",
    "next-bundle-analyzer": "^0.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
