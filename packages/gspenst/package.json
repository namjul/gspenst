{
  "name": "gspenst",
  "version": "0.1.3",
  "license": "MIT",
  "exports": {
    "./data": {
      "module": "./data/dist/gspenst-data.esm.js",
      "default": "./data/dist/gspenst-data.cjs.js"
    },
    "./mdx": {
      "module": "./mdx/dist/gspenst-mdx.esm.js",
      "default": "./mdx/dist/gspenst-mdx.cjs.js"
    },
    ".": {
      "module": "./dist/gspenst.esm.js",
      "default": "./dist/gspenst.cjs.js"
    },
    "./schema": {
      "module": "./schema/dist/gspenst-schema.esm.js",
      "default": "./schema/dist/gspenst-schema.cjs.js"
    },
    "./server": {
      "module": "./server/dist/gspenst-server.esm.js",
      "default": "./server/dist/gspenst-server.cjs.js"
    },
    "./package.json": "./package.json",
    "./pageMap.json": "./pageMap.json"
  },
  "main": "dist/gspenst.cjs.js",
  "module": "dist/gspenst.esm.js",
  "files": [
    "dist",
    "server",
    "schema",
    "data",
    "mdx",
    "routingMapping.json"
  ],
  "scripts": {
    "dev": "tinacms dev --rootPath src",
    "lint:types": "tsc --noEmit",
    "test": "tinacms audit --rootPath src"
  },
  "dependencies": {
    "@napi-rs/simple-git": "^0.1.8",
    "@tinacms/cli": "^1.5.25",
    "@tryghost/nql": "^0.9.1",
    "async-mutex": "^0.3.2",
    "cross-spawn": "^7.0.3",
    "dataloader": "^2.1.0",
    "debug": "^4.3.3",
    "deepmerge": "^4.2.2",
    "dot-prop": "^6.0.1",
    "fnv1a": "^1.0.0",
    "fs-extra": "^10.0.0",
    "graphql": "^16.6.0",
    "ioredis": "^5.0.3",
    "neverthrow": "^6.0.0",
    "normalizr": "^3.6.2",
    "path-to-regexp": "^6.2.0",
    "react-is": ">=16.13.1",
    "slugify": "^1.6.5",
    "sort-on": "^4.0.0",
    "styled-components": "^5.3.5",
    "tinacms": "^1.5.16",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/mock-fs": "^4.13.1",
    "mock-fs": "^5.1.1",
    "type-fest": "^2.8.0"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "preconstruct": {
    "exports": {
      "extra": {
        "./pageMap.json": "./pageMap.json"
      }
    },
    "entrypoints": [
      "index.ts",
      "server.ts",
      "schema.ts",
      "data.ts",
      "mdx.tsx"
    ]
  }
}
