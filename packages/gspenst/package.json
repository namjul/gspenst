{
  "name": "gspenst",
  "version": "0.0.6",
  "license": "MIT",
  "exports": {
    "./data": {
      "module": "./data/dist/gspenst-data.esm.js",
      "default": "./data/dist/gspenst-data.cjs.js"
    },
    ".": {
      "module": "./dist/gspenst.esm.js",
      "default": "./dist/gspenst.cjs.js"
    },
    "./server": {
      "module": "./server/dist/gspenst-server.esm.js",
      "default": "./server/dist/gspenst-server.cjs.js"
    },
    "./components/MdxTheme": {
      "module": "./components/MdxTheme/dist/gspenst-components-MdxTheme.esm.js",
      "default": "./components/MdxTheme/dist/gspenst-components-MdxTheme.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/gspenst.cjs.js",
  "module": "dist/gspenst.esm.js",
  "files": [
    "dist",
    "server",
    "data",
    "components"
  ],
  "scripts": {
    "dev": "tinacms dev --rootPath src",
    "lint:types": "tsc --noEmit",
    "test:tina": "tinacms audit --rootPath src"
  },
  "dependencies": {
    "@napi-rs/simple-git": "^0.1.8",
    "@tryghost/nql": "^0.9.1",
    "async-mutex": "^0.3.2",
    "cross-spawn": "^7.0.3",
    "dataloader": "^2.1.0",
    "debug": "^4.3.3",
    "deepmerge": "^4.2.2",
    "dot-prop": "^6.0.1",
    "filter-obj": "^2.0.0",
    "fnv1a": "^1.0.0",
    "fs-extra": "^10.0.0",
    "graphql": "^16.6.0",
    "ioredis": "^5.0.3",
    "neverthrow": "^4.3.1",
    "normalizr": "^3.6.2",
    "path-to-regexp": "^6.2.0",
    "react-is": ">=16.13.1",
    "slugify": "^1.6.5",
    "sort-on": "^4.0.0",
    "styled-components": "^5.3.5",
    "tinacms": "^1.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tinacms/cli": "^1.5.5",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/mock-fs": "^4.13.1",
    "mock-fs": "^5.1.1",
    "type-fest": "^2.8.0"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "preconstruct": {
    "exports": true,
    "entrypoints": [
      "index.ts",
      "server.ts",
      "data.ts",
      "components/MdxTheme.tsx"
    ]
  }
}
